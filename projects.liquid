title: projects
layout: default.liquid
data:
    route: projects
---
<ul class="project-list main-list">
    {% for post in collections.posts.pages %}
        {% if post.data.type == "project" %}
            <li>
                <a href="{{ post.permalink }}">{{ post.title }}</a>
            </li>
        {% endif %}
    {% endfor %}
</ul>
